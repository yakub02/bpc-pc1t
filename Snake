#include <stdio.h>
#include <time.h>
#include <stdlib.h>
#include <conio.h>
#include<time.h>
#include<ctype.h>
#include <time.h>
#include <windows.h>
#include <process.h>

#define N 20
#define M 40

int i, j;

void border()
{
	for (i = 0; i <= M + 1; i++)
	{
		printf("#");

	}
	printf("\n");
	for (i = 0; i <= N + 1; i++)
	{
		printf("#");
		for (j = 0; j <= M + 1; j++)
		{
			if (j == M - 1)
			{
				printf("#\n");
			}
			else
			{
				printf(" ");
			}
		}
	}
	
	for (i = 0; i <= M + 1; i++)
	{
		printf("#");
	}
}
void main()
{
	border();
}
